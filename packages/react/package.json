{
  "name": "@yamada-ui/react",
  "type": "module",
  "version": "1.7.7",
  "description": "React UI components of the Yamada, by the Yamada, for the Yamada built with React and Emotion",
  "keywords": [
    "yamada",
    "yamada-ui",
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library",
    "design-system"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "scripts": {
    "clean": "rimraf node_modules dist trace coverage storybook-static .turbo .eslintcache",
    "prebuild": "tsx scripts/pre-build.ts",
    "build": "tsdown",
    "postbuild": "tsx scripts/post-build.ts",
    "storybook": "storybook dev -p 6006 -c storybook",
    "storybook:serve": "npx http-server ./storybook-static",
    "storybook:build": "storybook build -c storybook",
    "test": "cross-env NODE_OPTIONS=\"--experimental-strip-types\" vitest",
    "test:dev": "cross-env NODE_OPTIONS=\"--experimental-strip-types\" vitest --watch --ui",
    "typecheck": "tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "typeanalyze": "npx analyze-trace ./trace --expandTypes=false",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/sheet": "^1.4.0",
    "@emotion/styled": "^11.14.1",
    "@emotion/use-insertion-effect-with-fallbacks": "^1.2.0",
    "@emotion/utils": "^1.4.2",
    "@floating-ui/react-dom": "^2.1.4",
    "@tanstack/react-table": "^8.21.3",
    "@yamada-ui/utils": "workspace:*",
    "color2k": "^2.0.3",
    "copy-to-clipboard": "3.3.3",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "file-selector": "^2.1.2",
    "framesync": "6.1.2",
    "intl-messageformat": "^10.7.16",
    "lucide-react": "0.533.0",
    "motion": "12.18.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-fast-compare": "^3.2.2",
    "react-focus-lock": "^2.13.6",
    "react-remove-scroll": "^2.7.1",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@lucide/lab": "^0.1.2",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "clean-package": "2.2.0",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "use-mask-input": "^3.5.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "motion": ">=12",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "clean-package": "./clean-package.config.json"
}
