{
  "name": "@yamada-ui/workspace",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "The workspace for Yamada UI projects",
  "scripts": {
    "clean": "rimraf .eslintcache",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache"
  },
  "exports": {
    "./eslint": "./src/eslint/index.ts",
    "./prettier": "./src/prettier/index.ts",
    "./prettier/*": "./src/prettier/*.js",
    "./typescript/*": "./src/typescript/*",
    "./vitest/*": "./src/vitest/*.ts",
    "./octokit": "./src/octokit/index.ts"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^9.1.3",
    "@eslint/compat": "^1.3.1",
    "@octokit/request-error": "^7.0.0",
    "@octokit/rest": "^22.0.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "dotenv": "^16.6.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vitest-axe": "^0.1.0"
  }
}
