{
  "SimpleGrid": {
    "as": {
      "type": "As",
      "description": "The HTML element to render.",
      "required": false
    },
    "asChild": {
      "type": "boolean",
      "description": "Merges its props onto its immediate child.",
      "required": false
    },
    "css": {
      "type": "CSSObject | CSSObject[]",
      "description": "The CSS object.",
      "required": false
    },
    "colorScheme": {
      "type": "\"amber\" | \"black\" | \"blackAlpha\" | \"blue\" | \"cyan\" | \"danger\" | \"emerald\" | \"error\" | \"flashy\" | \"fuchsia\" ...",
      "description": "Set color scheme variables.",
      "required": false
    },
    "area": {
      "type": "\"-moz-initial\" | \"auto\" | \"inherit\" | \"initial\" | \"revert-layer\" | \"revert\" | \"unset\" | number & {} ...",
      "description": "The CSS `grid-area` property.",
      "required": false
    },
    "autoColumns": {
      "type": "\"-moz-initial\" | \"0.5\" | \"1.5\" | \"1\" | \"1/12\" | \"1/2\" | \"1/3\" | \"1/4\" | \"1/5\" | \"1/6\" ...",
      "description": "The CSS `grid-auto-columns` property.",
      "required": false
    },
    "autoFlow": {
      "type": "\"-moz-initial\" | \"column\" | \"dense\" | \"inherit\" | \"initial\" | \"revert-layer\" | \"revert\" | \"row\" | \"unset\" ...",
      "description": "The CSS `grid-auto-flow` property.",
      "required": false
    },
    "autoRows": {
      "type": "\"-moz-initial\" | \"0.5\" | \"1.5\" | \"1\" | \"1/12\" | \"1/2\" | \"1/3\" | \"1/4\" | \"1/5\" | \"1/6\" ...",
      "description": "The CSS `grid-auto-rows` property.",
      "required": false
    },
    "column": {
      "type": "\"-moz-initial\" | \"auto\" | \"inherit\" | \"initial\" | \"revert-layer\" | \"revert\" | \"unset\" | number & {} ...",
      "description": "The CSS `grid-column` property.",
      "required": false
    },
    "columns": {
      "type": "number",
      "description": "The number of columns.",
      "required": false
    },
    "minChildWidth": {
      "type": "\"-moz-fit-content\" | \"-moz-initial\" | \"-moz-max-content\" | \"-moz-min-content\" | \"-webkit-fill-available\" | \"-webkit-fit-content\" | \"-webkit-max-content\" | \"-webkit-min-content\" | \"0.5\" | \"1.5\" ...",
      "description": "The width at which child elements will break into columns.\nPass a number for pixel values or a string for any other valid CSS length.",
      "required": false
    },
    "row": {
      "type": "\"-moz-initial\" | \"auto\" | \"inherit\" | \"initial\" | \"revert-layer\" | \"revert\" | \"unset\" | number & {} ...",
      "description": "The CSS `grid-row` property.",
      "required": false
    },
    "templateAreas": {
      "type": "\"-moz-initial\" | \"inherit\" | \"initial\" | \"none\" | \"revert-layer\" | \"revert\" | \"unset\" ...",
      "description": "The CSS `grid-template-areas` property.",
      "required": false
    },
    "templateColumns": {
      "type": "\"-moz-initial\" | \"0.5\" | \"1.5\" | \"1\" | \"1/12\" | \"1/2\" | \"1/3\" | \"1/4\" | \"1/5\" | \"1/6\" ...",
      "description": "The CSS `grid-template-columns` property.",
      "required": false
    },
    "templateRows": {
      "type": "\"-moz-initial\" | \"0.5\" | \"1.5\" | \"1\" | \"1/12\" | \"1/2\" | \"1/3\" | \"1/4\" | \"1/5\" | \"1/6\" ...",
      "description": "The CSS `grid-template-rows` property.",
      "required": false
    }
  }
}
