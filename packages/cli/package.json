{
  "name": "@yamada-ui/cli",
  "type": "module",
  "version": "2.0.5",
  "description": "The official CLI for Yamada UI projects",
  "keywords": [
    "theme",
    "theming",
    "typings",
    "generator",
    "yamada ui",
    "yamada",
    "ui mode",
    "ui"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "scripts": {
    "clean": "rimraf node_modules dist trace coverage .turbo .eslintcache",
    "dev": "tsx src/index.ts",
    "build": "tsdown",
    "typecheck": " tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "typeanalyze": "npx analyze-trace ./trace --expandTypes=false",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@yamada-ui/utils": "workspace:*",
    "boxen": "^8.0.1",
    "cli-check-node": "^1.3.4",
    "commander": "catalog:",
    "diff": "^8.0.2",
    "esbuild": "^0.25.11",
    "eslint": "catalog:",
    "execa": "catalog:",
    "glob": "catalog:",
    "https-proxy-agent": "^7.0.6",
    "listr2": "^9.0.5",
    "node-eval": "^2.0.0",
    "node-fetch": "catalog:",
    "ora": "catalog:",
    "picocolors": "catalog:",
    "prettier": "catalog:",
    "prompts": "^2.4.2",
    "rimraf": "catalog:",
    "semver": "^7.7.3",
    "sucrase": "catalog:",
    "validate-npm-package-name": "^6.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "@types/update-notifier": "6.0.8",
    "@types/validate-npm-package-name": "^4.0.2",
    "@types/yamljs": "^0.2.34",
    "clean-package": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "update-notifier": "^7.3.1"
  },
  "clean-package": "./clean-package.config.json"
}
