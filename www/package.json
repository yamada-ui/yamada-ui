{
  "name": "www",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "clean": "rimraf .next .velite .eslintcache tsconfig.tsbuildinfo node_modules",
    "dev:content": "NODE_OPTIONS=\"--experimental-strip-types\" velite --watch",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "build:content": "NODE_OPTIONS=\"--experimental-strip-types\" velite --clean",
    "build:next": "next build",
    "build": "run-s build:*",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../.prettierignore --check --cache",
    "gen:icons": "tsx scripts/generate/icons",
    "gen:contributors": "tsx scripts/generate/contributors"
  },
  "dependencies": {
    "@lucide/lab": "^0.1.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@yamada-ui/react": "workspace:*",
    "match-sorter": "^8.0.3",
    "motion": "12.18.1",
    "next": "^15.4.1",
    "next-intl": "^4.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "shiki": "3.3.0"
  },
  "devDependencies": {
    "sucrase": "^3.35.0",
    "rimraf": "^6.0.1",
    "@next/eslint-plugin-next": "^15.4.1",
    "@shikijs/rehype": "^3.8.0",
    "@shikijs/transformers": "^3.8.0",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "@yamada-ui/workspace": "workspace:*",
    "dotenv": "^17.2.0",
    "execa": "9.3.1",
    "hastscript": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-directive": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "velite": "^0.2.4"
  }
