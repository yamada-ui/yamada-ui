{
  "name": "@yamada-ui/use-breakpoint",
  "version": "1.3.0",
  "description": "Yamada UI useBreakpoint custom hook",
  "keywords": [
    "use-breakpoint"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/hooks/use-breakpoint"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "scripts": {
    "dev": "pnpm build:fast -- --watch",
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@yamada-ui/core": "workspace:*",
    "@yamada-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "clean-package": "2.2.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "banner": {
      "js": "\"use client\""
    },
    "sourcemap": true
  }
}
