{
  "name": "yamada-ui",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.0",
  "license": "MIT",
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui"
  },
  "scripts": {
    "prepare": "lefthook install",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log .eslintcache coverage node_modules",
    "clean:all": "pnpm clean && pnpm rimraf .turbo *.log .eslintcache coverage storybook-static trace node_modules",
    "storybook": "pnpm storybook:react",
    "storybook:react": "pnpm --prefix packages/react storybook",
    "storybook:serve": "pnpm --prefix packages/react storybook:serve",
    "storybook:build": "pnpm --prefix packages/react storybook:build",
    "dev:docs": "pnpm --prefix docs dev",
    "dev:next-pages": "pnpm --prefix examples/next/pages dev",
    "dev:next-app": "pnpm --prefix examples/next/app dev",
    "dev:vite": "pnpm --prefix examples/vite dev",
    "dev:remix": "pnpm --prefix examples/remix dev",
    "dev:hono": "pnpm --prefix examples/hono dev",
    "prebuild": "pnpm build:cli && pnpm gen:tokens",
    "build": "turbo run build --filter=./packages/**",
    "build:cli": "pnpm --prefix packages/cli build",
    "build:react": "pnpm --prefix packages/react build",
    "build:utils": "pnpm --prefix packages/utils build",
    "build:docs": "pnpm --prefix docs build",
    "lint": "turbo run lint --filter=./packages/**",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "lint:cli": "pnpm --prefix packages/cli lint",
    "lint:react": "pnpm --prefix packages/react lint",
    "lint:utils": "pnpm --prefix packages/utils lint",
    "lint:docs": "pnpm --prefix docs lint",
    "format": "turbo run format --filter=./packages/**",
    "format:cli": "pnpm --prefix packages/cli format",
    "format:react": "pnpm --prefix packages/react format",
    "format:utils": "pnpm --prefix packages/utils format",
    "format:docs": "pnpm --prefix docs format",
    "typecheck": "turbo run typecheck --filter=./packages/**",
    "typecheck:cli": "pnpm --prefix packages/cli typecheck",
    "typecheck:react": "pnpm --prefix packages/react typecheck",
    "typecheck:utils": "pnpm --prefix packages/utils typecheck",
    "typecheck:docs": "pnpm --prefix docs typecheck",
    "test": "turbo run test --filter=./packages/**",
    "test:react": "pnpm --prefix packages/react test",
    "test:utils": "pnpm --prefix packages/utils test",
    "test:docs": "pnpm --prefix docs test",
    "test:issue": "tsx scripts/coverage-issues",
    "spellcheck": "cspell \"**/*.{js,jsx,mjs,ts,tsx,mts,md,mdx}\" --quiet",
    "quality": "pnpm format && pnpm lint && pnpm typecheck && pnpm test",
    "gen:styles": "tsx scripts/generate/styles",
    "gen:tokens": "yamada-cli tokens packages/react/src/theme/index.ts --internal --out packages/react/src/core/generated-theme-tokens.types.ts && eslint packages/react/src/core/generated-theme-tokens.types.ts --fix",
    "gen:icons": "tsx scripts/generate/icons",
    "gen:props": "tsx scripts/generate/props",
    "changelog": "tsx scripts/changelog",
    "changelog:ci": "tsx scripts/changelog --current",
    "changelog:latest": "tsx scripts/changelog --latest",
    "version:dev": "changeset version --snapshot dev",
    "version:next": "changeset version --snapshot next",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "release:next": "changeset publish --tag next",
    "report:insights": "tsx scripts/report-insights",
    "notification:release": "tsx scripts/notification-release"
  },
  "dependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.3",
    "@changesets/get-release-plan": "^4.0.11",
    "@changesets/types": "^6.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/eslint-plugin": "^9.0.1",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@eslint/compat": "^1.2.9",
    "@faker-js/faker": "^9.7.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@koralle/vitest-axe": "^0.1.1",
    "@lucide/lab": "^0.1.2",
    "@mdn/browser-compat-data": "^6.0.13",
    "@octokit/request-error": "^6.1.8",
    "@octokit/rest": "^21.1.1",
    "@storybook/addon-a11y": "^9.0.4",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/cli": "^9.0.4",
    "@storybook/react-vite": "^9.0.4",
    "@storybook/test-runner": "^0.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__traverse": "^7.20.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/parser": "^8.32.0",
    "@typescript/analyze-trace": "^0.10.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/eslint-plugin": "^1.1.44",
    "@vitest/ui": "^3.1.3",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "axe-playwright": "^2.1.0",
    "axios": "^1.9.0",
    "commander": "^13.1.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "execa": "9.3.1",
    "find-packages": "^10.0.4",
    "glob": "^11.0.2",
    "globals": "^16.1.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.12",
    "lucide-react": "^0.509.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.4",
    "storybook-dark-mode": "^4.0.2",
    "storybook-i18n": "^4.0.4",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vitest-matchmedia-mock": "^1.0.6",
    "web-features": "^2.34.2"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  },
  "devDependencies": {
    "eslint-plugin-storybook": "9.0.4"
  }
}
