{
  "name": "@yamada-ui/workspace",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "The workspace for Yamada UI projects",
  "scripts": {
    "clean": "rimraf node_modules trace .eslintcache",
    "typecheck": "tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "typeanalyze": "npx analyze-trace ./trace --expandTypes=false",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache"
  },
  "exports": {
    "./eslint": "./src/eslint/index.ts",
    "./prettier": "./src/prettier/index.ts",
    "./prettier/*": "./src/prettier/*.js",
    "./typescript/*": "./src/typescript/*",
    "./vitest/*": "./src/vitest/*.ts",
    "./octokit": "./src/octokit/index.ts",
    "./codecov": "./src/codecov/index.ts",
    "./discord": "./src/discord/index.ts"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^9.6.4",
    "@eslint/compat": "^2.0.2",
    "@limegrass/eslint-plugin-import-alias": "^1.6.0",
    "@octokit/request-error": "^7.1.0",
    "@octokit/rest": "^22.0.0",
    "@octokit/webhooks": "^14.2.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/eslint-plugin": "^1.6.9",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0"
  }
}
