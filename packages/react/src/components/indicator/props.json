{
  "Indicator": {
    "as": {
      "type": "As",
      "description": "The HTML element to render.",
      "required": false
    },
    "asChild": {
      "type": "boolean",
      "description": "Merges its props onto its immediate child.",
      "required": false
    },
    "css": {
      "type": "CSSObject | CSSObject[]",
      "description": "The CSS object.",
      "required": false
    },
    "colorScheme": {
      "type": "\"amber\" | \"black\" | \"blackAlpha\" | \"blue\" | \"cyan\" | \"danger\" | \"emerald\" | \"error\" | \"flashy\" | \"fuchsia\" ...",
      "description": "Set color scheme variables.",
      "required": false
    },
    "size": {
      "type": "\"lg\" | \"md\" | \"sm\"",
      "defaultValue": "hoge",
      "description": "The size of the component.",
      "required": false
    },
    "variant": {
      "type": "\"solid\" | \"subtle\" | \"surface\"",
      "defaultValue": "hoge",
      "description": "The variant of the component.",
      "required": false
    },
    "disabled": {
      "type": "boolean",
      "defaultValue": "false",
      "description": "If `true`, the indicator will be disabled.",
      "required": false
    },
    "floatProps": {
      "type": "IndicatorFloatProps",
      "description": "Props for the float element.",
      "required": false
    },
    "inline": {
      "type": "boolean",
      "defaultValue": "false",
      "description": "If `true`, set the indicator as an inline element.",
      "required": false
    },
    "label": {
      "type": "ReactNode",
      "description": "The indicator label to use.",
      "required": false
    },
    "labelProps": {
      "type": "IndicatorLabelProps",
      "description": "Props for the label element.",
      "required": false
    },
    "offset": {
      "type": "\"-0.5\" | \"-1.5\" | \"-1\" | \"-10\" | \"-11\" | \"-12\" | \"-13\" | \"-14\" | \"-15\" | \"-16\" ...",
      "description": "Changes position offset.",
      "required": false
    },
    "overflowCount": {
      "type": "number",
      "defaultValue": "99",
      "description": "If `label` is of type number, the maximum number displayed.",
      "required": false
    },
    "ping": {
      "type": "boolean | PingOptions",
      "defaultValue": "false",
      "description": "If `true`, make an element scale and fade like a radar ping or ripple of water.",
      "required": false
    },
    "placement": {
      "type": "\"center-center\" | \"center-end\" | \"center-start\" | \"center\" | \"end-center\" | \"end-end\" | \"end-start\" | \"end\" | \"start-center\" | \"start-end\" ...",
      "defaultValue": "\"start-end\"",
      "description": "The placement of the float.",
      "required": false
    },
    "showZero": {
      "type": "boolean",
      "defaultValue": "true",
      "description": "If `true`, display 0.",
      "required": false
    },
    "withBorder": {
      "type": "boolean",
      "defaultValue": "false",
      "description": "If `true`, display the border of the indicator.",
      "required": false
    }
  }
}
