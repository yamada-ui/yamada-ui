{
  "name": "@yamada-ui/react",
  "type": "module",
  "version": "1.7.3",
  "description": "React UI components of the Yamada, by the Yamada, for the Yamada built with React and Emotion",
  "keywords": [
    "yamada",
    "yamada-ui",
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library",
    "design-system"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "scripts": {
    "clean": "rimraf dist .turbo .eslintcache",
    "build": "tsup",
    "storybook": "storybook dev -p 6006 -c storybook",
    "storybook:serve": "npx http-server ../../storybook-static",
    "storybook:build": "storybook build -c storybook -o ../../storybook-static",
    "test": "vitest",
    "test:dev": "vitest --watch --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache --flag unstable_ts_config",
    "format": "prettier --ignore-path ../../.prettierignore --check . --cache",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@emotion/css": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/sheet": "1.4.0",
    "@emotion/styled": "11.11.0",
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-table": "^8.20.6",
    "@yamada-ui/utils": "workspace:*",
    "copy-to-clipboard": "3.3.3",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.5.1",
    "embla-carousel-react": "^8.5.1",
    "framesync": "6.1.2",
    "lucide-react": "0.464.0",
    "motion": "^11.15.0",
    "react-dropzone-esm": "^15.2.0",
    "react-fast-compare": "^3.2.2",
    "react-focus-lock": "^2.13.5",
    "react-remove-scroll": "^2.6.2",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "clean-package": "2.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "motion": ">=11",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "clean-package": "../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "dts": true,
    "bundle": false,
    "target": "es2022",
    "format": [
      "cjs",
      "esm"
    ],
    "entry": [
      "src",
      "!src/**/*.test.*",
      "!src/**/*.stories.*"
    ],
    "sourcemap": true
  }
}
