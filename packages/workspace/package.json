{
  "name": "@yamada-ui/workspace",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "The workspace for Yamada UI projects",
  "scripts": {
    "clean": "rimraf node_modules trace .eslintcache",
    "typecheck": "tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "typeanalyze": "npx analyze-trace ./trace --expandTypes=false",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache"
  },
  "exports": {
    "./eslint": "./src/eslint/index.ts",
    "./prettier": "./src/prettier/index.ts",
    "./prettier/*": "./src/prettier/*.js",
    "./typescript/*": "./src/typescript/*",
    "./vitest/*": "./src/vitest/*.ts",
    "./octokit": "./src/octokit/index.ts",
    "./codecov": "./src/codecov/index.ts",
    "./discord": "./src/discord/index.ts"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^9.2.1",
    "@eslint/compat": "^1.4.0",
    "@limegrass/eslint-plugin-import-alias": "^1.6.0",
    "@octokit/request-error": "^7.0.1",
    "@octokit/rest": "^22.0.0",
    "@octokit/webhooks": "^14.1.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/eslint-plugin": "^1.3.23",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-testing-library": "^7.13.3",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript-eslint": "^8.46.2",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  }
}
