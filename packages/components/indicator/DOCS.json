{
  "Indicator": {
    "colorScheme": {
      "type": "\"blackAlpha\" | \"whiteAlpha\" | \"amber\" | \"blue\" | \"info\" | \"link\" | \"primary\" | \"cyan\" | \"emerald\" | \"flashy\" | \"fuchsia\" | \"gray\" | \"green\" | \"success\" | \"indigo\" | \"lime\" | \"neutral\" | \"orange\" | \"warning\" | \"pink\" | \"purple\" | \"red\" | \"danger\" | \"rose\" | \"sky\" | \"teal\" | \"violet\" | \"secondary\" | \"yellow\"",
      "defaultValue": "\"primary\"",
      "description": "The visual color appearance of the component.",
      "required": false
    },
    "containerProps": {
      "type": "Omit<HTMLUIProps, \"children\">",
      "description": "Props for indicator wrapper element.",
      "required": false
    },
    "inline": {
      "type": "UIValue<boolean>",
      "defaultValue": false,
      "description": "If `true`, set the indicator as an inline element.",
      "required": false
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "description": "If `true`, the indicator will be disabled.",
      "required": false
    },
    "label": {
      "type": "type ONLY_FOR_FORMAT =\n  | string\n  | number\n  | boolean\n  | ReactElement<any, string | JSXElementConstructor<any>>\n  | Iterable<ReactNode>\n  | ReactPortal",
      "description": "The indicator label to use.",
      "required": false
    },
    "offset": {
      "type": "UIValue<number>",
      "defaultValue": "0",
      "description": "Changes position offset, usually used when element has border-radius.",
      "required": false
    },
    "overflowCount": {
      "type": "number",
      "defaultValue": "99",
      "description": "If `label` is of type number, the maximum number displayed.",
      "required": false
    },
    "ping": {
      "type": "boolean",
      "defaultValue": false,
      "description": "If `true`, make an element scale and fade like a radar ping or ripple of water.",
      "required": false
    },
    "pingColor": {
      "type": "UIValue<\"border\" | ({} & string) | BackgroundColor | \"amber.50\" | \"amber.100\" | \"amber.200\" | \"amber.300\" | \"amber.400\" | \"amber.500\" | \"amber.600\" | \"amber.700\" | ... 320 more ... | \"yellow.950\">",
      "defaultValue": "var(--ui-ping)",
      "description": "It is used for the color of the ping animation.",
      "required": false
    },
    "pingCount": {
      "type": "AnimationIterationCount",
      "defaultValue": "infinite",
      "description": "It is used for the count of the ping animation.",
      "required": false
    },
    "pingDuration": {
      "type": "AnimationDirection",
      "defaultValue": "1.4s",
      "description": "It is used for the duration of the ping animation.",
      "required": false
    },
    "pingScale": {
      "type": "number",
      "defaultValue": "1.8",
      "description": "It is used for the scale of the ping animation.",
      "required": false
    },
    "placement": {
      "type": "UIValue<\n  | \"bottom\"\n  | \"left\"\n  | \"right\"\n  | \"top\"\n  | \"bottom-left\"\n  | \"bottom-right\"\n  | \"top-left\"\n  | \"top-right\"\n>",
      "defaultValue": "'top-right'",
      "description": "The placement of the indicator.",
      "required": false
    },
    "showZero": {
      "type": "boolean",
      "defaultValue": true,
      "description": "If `true`, display 0.",
      "required": false
    },
    "size": {
      "type": "\"sm\" | \"md\" | \"lg\"",
      "defaultValue": "\"md\"",
      "description": "The size of the Indicator.",
      "required": false
    },
    "variant": {
      "type": "\"solid\" | \"subtle\"",
      "defaultValue": "\"solid\"",
      "description": "The variant of the Indicator.",
      "required": false
    },
    "withBorder": {
      "type": "boolean",
      "defaultValue": false,
      "description": "If `true`, display the border of the indicator.",
      "required": false
    }
  }
}
