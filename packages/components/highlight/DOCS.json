{
  "Highlight": {
    "query": {
      "type": "string | string[]",
      "description": "Can be a single string or an array of strings. These are the terms that are highlighted in the text.",
      "required": true
    },
    "align": {
      "type": "UIValue<TextAlign>",
      "description": "The CSS `text-align` property.",
      "required": false
    },
    "casing": {
      "type": "UIValue<TextTransform>",
      "description": "The CSS `text-transform` property.",
      "required": false
    },
    "decoration": {
      "type": "UIValue<TextDecoration<0 | (string & {})>>",
      "description": "The CSS `text-decoration` property.",
      "required": false
    },
    "fragment": {
      "type": "boolean",
      "defaultValue": false,
      "description": "If `true`, `Fragment` is used for rendering.",
      "required": false
    },
    "isFragment": {
      "type": "boolean",
      "defaultValue": false,
      "deprecated": "Use `fragment` instead.",
      "description": "If `true`, `Fragment` is used for rendering.",
      "required": false
    },
    "markProps": {
      "type": "MarkProps",
      "description": "Properties passed to the Mark component which is used to highlight the matched terms.",
      "required": false
    }
  },
  "Mark": {
    "colorScheme": {
      "type": "\"blackAlpha\" | \"whiteAlpha\" | \"amber\" | \"blue\" | \"info\" | \"link\" | \"primary\" | \"cyan\" | \"emerald\" | \"flashy\" | \"fuchsia\" | \"gray\" | \"green\" | \"success\" | \"indigo\" | \"lime\" | \"neutral\" | \"orange\" | \"warning\" | \"pink\" | \"purple\" | \"red\" | \"danger\" | \"rose\" | \"sky\" | \"teal\" | \"violet\" | \"secondary\" | \"yellow\"",
      "defaultValue": "\"secondary\"",
      "description": "The visual color appearance of the component.",
      "required": false
    },
    "size": {
      "type": "string",
      "description": "The size of the Mark.",
      "required": false
    },
    "variant": {
      "type": "\"outline\" | \"solid\" | \"subtle\" | \"text-accent\" | \"unstyled\"",
      "defaultValue": "\"subtle\"",
      "description": "The variant of the Mark.",
      "required": false
    }
  }
}
