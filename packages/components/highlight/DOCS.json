{
  "Highlight": {
    "query": {
      "type": "string | string[]",
      "required": true,
      "description": "Can be a single string or an array of strings. These are the terms that are highlighted in the text."
    },
    "align": {
      "type": "UIValue<TextAlign>",
      "required": false,
      "description": "The CSS `text-align` property."
    },
    "casing": {
      "type": "UIValue<TextTransform>",
      "required": false,
      "description": "The CSS `text-transform` property."
    },
    "decoration": {
      "type": "UIValue<TextDecoration<0 | (string & {})>>",
      "required": false,
      "description": "The CSS `text-decoration` property."
    },
    "isFragment": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, `Fragment` is used for rendering."
    },
    "markProps": {
      "type": "MarkProps",
      "required": false,
      "description": "Properties passed to the Mark component which is used to highlight the matched terms."
    }
  },
  "Mark": {
    "colorScheme": {
      "defaultValue": "secondary",
      "type": "\"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"neutral\" | \"red\" | \"rose\" | \"pink\" | \"flashy\" | \"orange\" | \"amber\" | \"yellow\" | \"lime\" | \"green\" | \"emerald\" | \"teal\" | \"cyan\" | \"sky\" | \"blue\" | \"indigo\" | \"violet\" | \"purple\" | \"fuchsia\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "size": {
      "type": "string",
      "required": false,
      "description": "The size of the Mark."
    },
    "variant": {
      "defaultValue": "subtle",
      "type": "\"solid\" | \"subtle\" | \"outline\" | \"text-accent\" | \"unstyled\"",
      "required": false,
      "description": "The variant of the Mark."
    }
  }
}
