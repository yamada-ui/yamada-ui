  },
  "devDependencies": {
    "@next/eslint-plugin-next": "catalog:",
    "@shikijs/rehype": "^3.13.0",
    "@shikijs/transformers": "^3.13.0",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "@yamada-ui/workspace": "workspace:*",
    "commander": "catalog:",
