      "required": false
    },
    "size": {
      "type": "\"lg\" | \"md\" | \"sm\"",
      "description": "The size of the component.",
      "required": false
    },
    "indicatorProps": {
      "type": "HTMLStyledProps",
      "description": "The props for the status indicator component",
      "required": false
    },
    "labelProps": {
      "type": "HTMLStyledProps",
      "description": "The props for the status label component",
      "required": false
    },
    "value": {
      "type": "StatusScheme",
