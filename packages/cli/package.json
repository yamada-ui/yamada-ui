{
  "name": "@yamada-ui/cli",
  "type": "module",
  "version": "1.2.4",
  "description": "The official CLI for Yamada UI projects",
  "keywords": [
    "theme",
    "theming",
    "typings",
    "generator",
    "yamada ui",
    "yamada",
    "ui mode",
    "ui"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "bin": {
    "yamada-cli": "bin/index.js"
  },
  "scripts": {
    "clean": "rimraf dist .turbo .eslintcache",
    "build": "tsup",
    "typecheck": " tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "chokidar": "^4.0.3",
    "cli-check-node": "^1.3.4",
    "cli-handle-unhandled": "^1.1.2",
    "commander": "^13.1.0",
    "esbuild": "^0.25.0",
    "glob": "^11.0.1",
    "node-eval": "^2.0.0",
    "prettier": "^3.5.1",
    "tar": "^7.4.3",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/update-notifier": "6.0.8",
    "chalk": "^5.4.1",
    "update-notifier": "^7.3.1"
  },
  "tsup": {
    "clean": true,
    "shims": true,
    "bundle": false,
    "format": [
      "esm"
    ],
    "target": "es2022",
    "entry": [
      "src"
    ]
  }
}
