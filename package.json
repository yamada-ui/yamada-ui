{
  "name": "yamada-ui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "license": "MIT",
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamada-ui/yamada-ui"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "cd packages/cli && pnpm build",
    "storybook": "storybook dev -p 6006",
    "serve:storybook": "npx http-server ./storybook-static",
    "build:storybook": "storybook build",
    "prebuild": "pnpm --prefix packages/cli build && pnpm gen:tokens",
    "build": "turbo run build --filter=./packages/*",
    "build:fast": "turbo run build:fast --filter=./packages/*",
    "dev": "turbo run dev --filter=./packages/*",
    "lint": "eslint packages/**/{src,tests}/** scripts/**/** vitest.config.mts --ext .js,.ts,.jsx,.tsx --max-warnings 0 --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check packages/**/** stories/**/** scripts/**/** --cache",
    "format:write": "prettier --write packages/**/** stories/**/** scripts/**/** --cache",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:issue": "tsx scripts/coverage-issues && tsx scripts/a11y-issues",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --coverage --watch --ui",
    "test:a11y": "test-storybook",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test",
    "gen:css": "tsx scripts/generate-css",
    "gen:tokens": "yamada-cli tokens packages/theme/src/index.ts --out packages/core/src/generated-theme.types.ts",
    "gen:component": "plop component && pnpm install",
    "gen:hook": "plop hook && pnpm install",
    "gen:docs": "pnpm --filter=./packages/components/** gen:docs",
    "changelog": "tsx scripts/changelog",
    "changelog:latest": "tsx scripts/changelog --latest",
    "version:dev": "changeset version --snapshot dev",
    "version:next": "changeset version --snapshot next",
    "observe:issue": "tsx scripts/observe-issues",
    "observe:pr": "tsx scripts/observe-pull-requests",
    "report:insight": "tsx scripts/report-insights",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "release:next": "changeset publish --tag next",
    "notification:discord": "tsx scripts/discord",
    "notification:mattermost": "tsx scripts/mattermost",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log node_modules",
    "docs:dev": "pnpm --prefix docs dev",
    "docs:start": "pnpm --prefix docs start",
    "docs:build": "pnpm --prefix docs build",
    "docs:lint": "pnpm --prefix docs lint",
    "docs:changelog": "pnpm --prefix docs gen:changelog",
    "docs:theme": "pnpm --prefix docs gen:theme",
    "docs:css": "pnpm --prefix docs gen:css",
    "docs:props": "pnpm --prefix docs gen:props",
    "docs:search": "pnpm --prefix docs gen:search",
    "docs:contributors": "pnpm --prefix docs gen:contributors",
    "docs:content": "pnpm --prefix docs gen:content",
    "docs:translate": "pnpm --prefix docs gen:translate",
    "docs:tokens": "pnpm --prefix docs gen:tokens",
    "react:start": "pnpm --prefix examples/create-react-app start",
    "react:build": "pnpm --prefix examples/create-react-app build",
    "next-pages:dev": "pnpm --prefix examples/nextjs-pages dev",
    "next-pages:build": "pnpm --prefix examples/nextjs-pages build",
    "next-pages:start": "pnpm --prefix examples/nextjs-pages start",
    "next-app:dev": "pnpm --prefix examples/nextjs-app dev",
    "next-app:build": "pnpm --prefix examples/nextjs-app build",
    "next-app:start": "pnpm --prefix examples/nextjs-app start",
    "vite:dev": "pnpm --prefix examples/vite dev",
    "vite:build": "pnpm --prefix examples/vite build",
    "vite:preview": "pnpm --prefix examples/vite preview",
    "remix:dev": "pnpm --prefix examples/remix dev",
    "remix:build": "pnpm --prefix examples/remix build",
    "remix:start": "pnpm --prefix examples/remix start",
    "hono:dev": "pnpm --prefix examples/hono dev",
    "hono:build": "pnpm --prefix examples/hono build"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@changesets/get-release-plan": "^4.0.0",
    "@changesets/types": "^6.0.0",
    "@clack/prompts": "^0.7.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@octokit/rest": "^20.1.0",
    "@storybook/addon-a11y": "^8.0.9",
    "@storybook/addon-backgrounds": "^8.0.9",
    "@storybook/addon-docs": "^8.0.9",
    "@storybook/addon-measure": "^8.0.9",
    "@storybook/addon-storysource": "^8.0.9",
    "@storybook/addon-viewport": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/cli": "^8.0.9",
    "@storybook/manager-api": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/theming": "^8.0.9",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.5.1",
    "@vitest/ui": "^1.5.1",
    "@yamada-ui/calendar": "workspace:*",
    "@yamada-ui/carousel": "workspace:*",
    "@yamada-ui/charts": "workspace:*",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/dropzone": "workspace:*",
    "@yamada-ui/fontawesome": "workspace:*",
    "@yamada-ui/markdown": "workspace:*",
    "@yamada-ui/nextjs": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/table": "workspace:*",
    "@yamada-ui/test": "workspace:*",
    "@yamada-ui/theme": "workspace:*",
    "axe-playwright": "^2.0.1",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.4.1",
    "execa": "^8.0.1",
    "find-packages": "^10.0.4",
    "glob": "^10.3.12",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "15.2.2",
    "list-it": "^1.3.12",
    "node-fetch": "^3.3.2",
    "plop": "4.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.1.0",
    "rimraf": "5.0.5",
    "storybook": "^8.0.9",
    "storybook-dark-mode": "^4.0.1",
    "tslib": "^2.6.2",
    "tsup": "8.0.2",
    "tsx": "^4.7.2",
    "turbo": "^1.13.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.1",
    "vitest-axe": "^0.1.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
