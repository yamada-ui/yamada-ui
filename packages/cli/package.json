{
  "name": "@yamada-ui/cli",
  "type": "module",
  "version": "2.0.5",
  "description": "The official CLI for Yamada UI projects",
  "keywords": [
    "theme",
    "theming",
    "typings",
    "generator",
    "yamada ui",
    "yamada",
    "ui mode",
    "ui"
  ],
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://yamada-ui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/yamada-ui/yamada-ui/issues"
  },
  "bin": {
    "yamada-cli": "dist/index.js"
  },
  "scripts": {
    "clean": "rimraf node_modules dist trace coverage .turbo .eslintcache",
    "dev": "tsx src/index.ts",
    "build": "tsdown",
    "typecheck": " tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "typeanalyze": "npx analyze-trace ./trace --expandTypes=false",
    "lint": "eslint . --max-warnings=0 --cache",
    "format": "prettier . --ignore-path ../../.prettierignore --check --cache"
  },
  "dependencies": {
    "@yamada-ui/utils": "workspace:*",
    "boxen": "^8.0.1",
    "cli-check-node": "^1.3.4",
    "commander": "^14.0.2",
    "diff": "^8.0.2",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "execa": "9.3.1",
    "glob": "^13.0.0",
    "https-proxy-agent": "^7.0.6",
    "listr2": "^9.0.5",
    "node-eval": "^2.0.0",
    "node-fetch": "^3.3.2",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.7.4",
    "prompts": "^2.4.2",
    "rimraf": "^6.1.2",
    "semver": "^7.7.3",
    "sucrase": "^3.35.1",
    "validate-npm-package-name": "^7.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "@types/update-notifier": "6.0.8",
    "@types/validate-npm-package-name": "^4.0.2",
    "@types/yamljs": "^0.2.34",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "update-notifier": "^7.3.1"
  }
}
