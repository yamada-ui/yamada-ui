{
  "Avatar": {
    "alt": {
      "type": "string",
      "required": false,
      "description": "The `HTMLImageElement` property `alt`."
    },
    "colorScheme": {
      "type": "\"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"neutral\" | \"red\" | \"rose\" | \"pink\" | \"flashy\" | \"orange\" | \"amber\" | \"yellow\" | \"lime\" | \"green\" | \"emerald\" | \"teal\" | \"cyan\" | \"sky\" | \"blue\" | \"indigo\" | \"violet\" | \"purple\" | \"fuchsia\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "format": {
      "type": "(name: string) => string",
      "required": false,
      "description": "Function to get the initials to display."
    },
    "icon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false,
      "description": "The avatar icon to use."
    },
    "ignoreFallback": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, opt out of the avatar's `fallback` logic and renders the `img` at all times."
    },
    "loading": {
      "type": "\"eager\" | \"lazy\"",
      "required": false,
      "description": "Defines loading strategy."
    },
    "name": {
      "type": "string",
      "required": false,
      "description": "The name of the person in the avatar.\n\n- If `src` has loaded, the name will be used as the `alt` attribute of the `img`\n- If `src` is not loaded, the name will be used to create the initials"
    },
    "referrerPolicy": {
      "type": "HTMLAttributeReferrerPolicy",
      "required": false,
      "description": "Defining which referrer is sent when fetching the resource."
    },
    "size": {
      "defaultValue": "md",
      "type": "\"2xs\" | \"xs\" | \"sm\" | \"md\" | \"lg\" | \"xl\" | \"2xl\"",
      "required": false,
      "description": "The size of the Avatar."
    },
    "src": {
      "type": "string",
      "required": false,
      "description": "The image url of the avatar."
    },
    "srcSet": {
      "type": "string",
      "required": false,
      "description": "List of sources to use for different screen resolutions."
    },
    "variant": {
      "type": "string",
      "required": false,
      "description": "The variant of the Avatar."
    }
  },
  "AvatarGroup": {
    "max": {
      "type": "number",
      "required": false,
      "description": "The maximum number of visible avatars."
    }
  },
  "AvatarBadge": {
    "ping": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, make an element scale and fade like a radar ping or ripple of water."
    },
    "pingColor": {
      "type": "UIValue<\"link\" | \"border\" | BackgroundColor | \"current\" | \"focus\" | \"whiteAlpha.50\" | \"whiteAlpha.100\" | \"whiteAlpha.200\" | \"whiteAlpha.300\" | \"whiteAlpha.400\" | ... 319 more ... | \"secondary.950\">",
      "defaultValue": "'[\"blackAlpha.400\", \"whiteAlpha.500\"]'",
      "required": false,
      "description": "It is used for the color of the ping animation."
    },
    "pingCount": {
      "type": "AnimationIterationCount",
      "defaultValue": "infinite",
      "required": false,
      "description": "It is used for the count of the ping animation."
    },
    "pingDuration": {
      "type": "AnimationDirection",
      "defaultValue": "1.4s",
      "required": false,
      "description": "It is used for the duration of the ping animation."
    },
    "pingScale": {
      "type": "number",
      "defaultValue": "1.8",
      "required": false,
      "description": "It is used for the scale of the ping animation."
    },
    "placement": {
      "type": "\"top-start\" | \"top-end\" | \"bottom-start\" | \"bottom-end\"",
      "defaultValue": "'bottom-end'",
      "required": false,
      "description": "The placement of the badge."
    }
  }
}
