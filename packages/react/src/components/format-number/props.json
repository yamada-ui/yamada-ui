{
  "FormatNumber": {
    "as": {
      "type": "As",
      "description": "The HTML element to render.",
      "required": false
    },
    "asChild": {
      "type": "boolean",
      "description": "Merges its props onto its immediate child.",
      "required": false
    },
    "css": {
      "type": "CSSObject | CSSObject[]",
      "description": "The CSS object.",
      "required": false
    },
    "colorScheme": {
      "type": "\"amber\" | \"black\" | \"blackAlpha\" | \"blue\" | \"cyan\" | \"danger\" | \"emerald\" | \"error\" | \"flashy\" | \"fuchsia\" ...",
      "description": "Set color scheme variables.",
      "required": false
    },
    "value": {
      "type": "number",
      "description": "The numeric value to be formatted.",
      "required": true
    },
    "locale": {
      "type": "string",
      "defaultValue": "\"en-US\"",
      "description": "The locale string to use for formatting.",
      "required": false
    }
  }
}
