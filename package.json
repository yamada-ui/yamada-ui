{
  "name": "yamada-ui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "packageManager": "pnpm@9.6.0+sha256.dae0f7e822c56b20979bb5965e3b73b8bdabb6b8b8ef121da6d857508599ca35",
  "license": "MIT",
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui"
  },
  "scripts": {
    "prepare": "lefthook install",
    "storybook": "storybook dev -p 6006",
    "serve:storybook": "npx http-server ./storybook-static",
    "build:storybook": "storybook build",
    "prebuild": "pnpm --prefix packages/cli build && pnpm gen:tokens",
    "build": "turbo run build --filter=./packages/**",
    "build:depend": "tsx scripts/dependency-build",
    "build:fast": "turbo run build:fast --filter=./packages/**",
    "dev": "turbo run dev --filter=./packages/**",
    "lint": "eslint . --max-warnings=0 --cache --flag unstable_ts_config",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "format": "prettier --write .",
    "format:check": "prettier --check packages/**/** stories/**/** scripts/**/** --cache",
    "format:write": "prettier --write packages/**/** stories/**/** scripts/**/** --cache",
    "typecheck": "tsc --noEmit",
    "typetrace": "tsc --noEmit --generateTrace ./trace --incremental false",
    "spellcheck": "cspell \"**/*.{js,jsx,mjs,ts,tsx,mts,md,mdx}\" --quiet",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:issue": "tsx scripts/coverage-issues && tsx scripts/a11y-issues",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --coverage --watch --ui",
    "test:a11y": "test-storybook",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test",
    "gen:css": "tsx scripts/generate-css && eslint packages/core/src/styles.ts --fix --flag unstable_ts_config",
    "gen:tokens": "yamada-cli tokens packages/theme/src/index.ts --out packages/core/src/generated-theme.types.ts && eslint packages/core/src/generated-theme.types.ts --fix --flag unstable_ts_config",
    "gen:component": "cross-env NODE_OPTIONS='--import tsx' plop component && pnpm install",
    "gen:hook": "cross-env NODE_OPTIONS='--import tsx' plop hook && pnpm install",
    "gen:docs": "pnpm --filter=./packages/components/** gen:docs",
    "changelog": "tsx scripts/changelog",
    "changelog:ci": "tsx scripts/changelog --current",
    "changelog:latest": "tsx scripts/changelog --latest",
    "version:dev": "changeset version --snapshot dev",
    "version:next": "changeset version --snapshot next",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "release:next": "changeset publish --tag next",
    "report:insights": "tsx scripts/report-insights",
    "notification:release": "tsx scripts/notification-release",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log node_modules",
    "clean:all": "pnpm clean && pnpm rimraf .turbo *.log .eslintcache coverage storybook-static trace node_modules",
    "docs:dev": "pnpm --prefix docs dev",
    "docs:start": "pnpm --prefix docs start",
    "docs:build": "pnpm --prefix docs build",
    "docs:lint": "pnpm --prefix docs lint",
    "docs:format": "pnpm --prefix docs format",
    "docs:format:check": "pnpm --prefix docs format:check",
    "docs:typecheck": "pnpm --prefix docs typecheck",
    "docs:test": "pnpm --prefix docs test",
    "docs:content": "pnpm --prefix docs gen:content",
    "docs:changelog": "pnpm --prefix docs gen:changelog",
    "docs:theme": "pnpm --prefix docs gen:theme",
    "docs:styled-system": "pnpm --prefix docs gen:styled-system",
    "docs:props": "pnpm --prefix docs gen:props",
    "docs:search": "pnpm --prefix docs gen:search",
    "docs:icon-tags": "pnpm --prefix docs gen:icon-tags",
    "docs:contributors": "pnpm --prefix docs gen:contributors",
    "docs:translate": "pnpm --prefix docs gen:translate",
    "docs:tokens": "pnpm --prefix docs gen:tokens",
    "docs:label": "pnpm --prefix docs gen:label",
    "react:start": "pnpm --prefix examples/create-react-app start",
    "react:build": "pnpm --prefix examples/create-react-app build",
    "next-pages:dev": "pnpm --prefix examples/next/pages dev",
    "next-pages:build": "pnpm --prefix examples/next/pages build",
    "next-pages:start": "pnpm --prefix examples/next/pages start",
    "next-app:dev": "pnpm --prefix examples/next/app dev",
    "next-app:build": "pnpm --prefix examples/next/app build",
    "next-app:start": "pnpm --prefix examples/next/app start",
    "vite:dev": "pnpm --prefix examples/vite dev",
    "vite:build": "pnpm --prefix examples/vite build",
    "vite:preview": "pnpm --prefix examples/vite preview",
    "remix:dev": "pnpm --prefix examples/remix dev",
    "remix:build": "pnpm --prefix examples/remix build",
    "remix:start": "pnpm --prefix examples/remix start",
    "hono:dev": "pnpm --prefix examples/hono dev",
    "hono:build": "pnpm --prefix examples/hono build"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@changesets/get-release-plan": "^4.0.4",
    "@changesets/types": "^6.0.0",
    "@clack/prompts": "^0.7.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@cspell/eslint-plugin": "^8.14.4",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@eslint/compat": "^1.1.1",
    "@faker-js/faker": "^9.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@koralle/vitest-axe": "^0.1.1",
    "@lucide/lab": "^0.1.2",
    "@octokit/request-error": "^6.1.5",
    "@octokit/rest": "^21.0.2",
    "@storybook/addon-a11y": "^8.3.3",
    "@storybook/addon-backgrounds": "^8.3.3",
    "@storybook/addon-docs": "^8.3.3",
    "@storybook/addon-measure": "^8.3.3",
    "@storybook/addon-storysource": "^8.3.3",
    "@storybook/addon-viewport": "^8.3.3",
    "@storybook/blocks": "^8.3.3",
    "@storybook/cli": "^8.3.3",
    "@storybook/manager-api": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.3.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@typescript/analyze-trace": "^0.10.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/eslint-plugin": "^1.1.4",
    "@vitest/ui": "^2.1.1",
    "@yamada-ui/calendar": "workspace:*",
    "@yamada-ui/carousel": "workspace:*",
    "@yamada-ui/charts": "workspace:*",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/core": "workspace:*",
    "@yamada-ui/dropzone": "workspace:*",
    "@yamada-ui/fontawesome": "workspace:*",
    "@yamada-ui/lucide": "workspace:*",
    "@yamada-ui/markdown": "workspace:*",
    "@yamada-ui/next": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/table": "workspace:*",
    "@yamada-ui/test": "workspace:*",
    "@yamada-ui/theme": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "axe-playwright": "^2.0.3",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "execa": "9.3.1",
    "find-packages": "^10.0.4",
    "glob": "^11.0.0",
    "globals": "^15.9.0",
    "jiti": "^2.0.0",
    "jsdom": "^25.0.1",
    "lefthook": "^1.7.17",
    "list-it": "^1.3.12",
    "lucide-react": "^0.446.0",
    "node-fetch": "^3.3.2",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.3.3",
    "storybook-dark-mode": "4.0.1",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
