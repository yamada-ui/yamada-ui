    "@lucide/lab": "catalog:",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@yamada-ui/react": "workspace:*",
    "match-sorter": "^8.1.0",
    "motion": "catalog:",
    "next": "catalog:",
    "next-intl": "^4.3.12",
    "prism-react-renderer": "^2.4.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
    "react-icons": "catalog:",
    "scroll-into-view-if-needed": "catalog:",
